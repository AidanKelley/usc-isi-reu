    sd:hasVersionId "v1.7.3"^^xsd:string ;
    sd:name "Speed improvements following benchmarks"^^xsd:string .

<https://example.org/objects/SoftwareVersion/empymod/v1.8.0> a sd:SoftwareVersion ;
    sd:author <https://example.org/objects/Person/prisae> ;
